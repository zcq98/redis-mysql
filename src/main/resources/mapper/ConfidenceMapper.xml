<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zcq.dao.ConfidenceMapper">
    <insert id="addConfidence">
        INSERT INTO mybatis.confidence (id, mac, ip, value)
        VALUES (#{id}, #{mac}, #{ip}, #{value})
    </insert>

    <delete id="deleteAll">
        DELETE
        FROM mybatis.confidence
    </delete>
</mapper>